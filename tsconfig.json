{
  "compilerOptions": {
    "target": "es2016",
    "module": "esnext",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "outDir": "./dist",
    "rootDir": "./src"
  },
  "buildOptions": {
    "outDir": "/dist",
    
  },
  "exclude": ["node_modules", "dist"],
  "include": ["src/index.ts"]
}
